{
  "expo": {
    "name": "UrTurn",
    "slug": "UrTurn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "urturn",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
      "eas": {
        "projectId": "5d52b67a-cba3-4d11-aece-b85039609ca1"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mrjuantapia.urturn",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Necesitamos tu ubicaci√≥n para mostrarte resultados cercanos y centrar el mapa.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "package": "com.mrjuantapia.urturn"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "meta": {
        "themeColor": "#ffffff",
        "viewport": "width=device-width, initial-scale=1, shrink-to-fit=no"
      },
      "display": "standalone",
      "shortName": "UrTurn",
      "backgroundColor": "#ffffff",
      "scope": "/",
      "startUrl": "/"
    },
    
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "owner": "mrjuantapia"
  }
}
